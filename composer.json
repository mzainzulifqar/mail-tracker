{
    "name": "zainzulifqargit/mail-tracker",
    "type": "library",
    "description": "Logs and tracks all outgoing emails from Laravel",
    "keywords": [
        "zainzulifqar",
        "MailTracker"
    ],
    "homepage": "https://github.com/zainzulifqargit/mail-tracker",
    "license": "MIT",
    "authors": [{
        "name": "Zain Zulifqar",
        "email": "zainzulifqar21@gmail.com",
        "homepage": "https://github.com/zainzulifqargit/mail-tracker",
        "role": "Developer"
    }],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.5",
        "aws/aws-php-sns-message-validator": "^1.1",
        "symfony/mailer": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "mockery/mockery": "^1.1"
    },
    "suggest": {
        "fedeisas/laravel-mail-css-inliner": "Automatically inlines CSS into all outgoing mail."
    },
    "autoload": {
        "psr-4": {
            "jdavidbakr\\MailTracker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jdavidbakr\\MailTracker\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "jdavidbakr\\MailTracker\\MailTrackerServiceProvider"
            ]
        }
    }
}
